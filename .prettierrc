{
	"semi": true,
	"trailingComma": "all",
	"singleQuote": true,
	"printWidth": 80,
	"arrowParens": "always",
	"tabWidth": 4,
	"useTabs": true,
	"plugins": ["prettier-plugin-astro", "prettier-plugin-tailwindcss"],
	"overrides": [
		{
			"files": "*.astro",
			"options": {
				"parser": "astro"
			}
		}
	]
}
